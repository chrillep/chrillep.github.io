    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@types/react": "17.0.58",
    "@types/react-dom": "17.0.19",
    "eslint": "7.32.0",
    "eslint-config-custom": "workspace:*",
    "jest": "26.6.3",
    "jest-presets": "workspace:*",
    "react": "18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "6.7.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "react-dom": "18.2.0"
  }
}
